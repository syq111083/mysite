CAAA;AAAA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,KAAA;AAAA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;;AAAA,YAAA,KAAA,CAAA,KAAA,KAAA,KAAA,CAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,MAAA;AACA,SAFA,MAEA;AACA,cAAA,GAAA,GAAA,eAAA;;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,QAAA,CAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,GAAA,MAAA;AACA,WAFA,MAEA;AAAA,YAAA,KAAA,CAAA,QAAA,GAAA,QAAA;AAAA;AACA;AACA,OAfA;;AAgBA,UAAA,KAAA,QAAA,KAAA,EAAA,EAAA;AAAA,aAAA,SAAA,GAAA,IAAA;AAAA,OAAA,MAAA;AACA,aAAA,SAAA,GAAA,KAAA;AACA;AACA,KArBA;AAsBA,IAAA,MAtBA,oBAsBA;AAAA;;AACA,WAAA,UAAA;;AACA,UAAA,KAAA,QAAA,KAAA,EAAA,IAAA,KAAA,KAAA,KAAA,EAAA,IAAA,KAAA,GAAA,KAAA,EAAA,IAAA,KAAA,QAAA,KAAA,EAAA,IAAA,KAAA,QAAA,KAAA,MAAA,EAAA;AACA,QAAA,KAAA,CAAA;AACA,UAAA,MAAA,EAAA,MADA;AAEA,UAAA,GAAA,EAAA,KAAA,OAAA,KAAA,OAAA,GAAA,cAFA;AAGA,UAAA,IAAA,EAAA;AACA,YAAA,QAAA,EAAA,KAAA,QADA;AAEA,YAAA,KAAA,EAAA,KAAA,QAFA;AAGA,YAAA,QAAA,EAAA,KAAA,QAHA;AAIA,YAAA,GAAA,EAAA,KAAA;AAJA;AAHA,SAAA,CAAA,CASA,IATA,CASA,UAAA,QAAA,EAAA;AACA,cAAA,GAAA,GAAA,EAAA;AACA,UAAA,GAAA,GAAA,QAAA,CAAA,IAAA;;AACA,cAAA,GAAA,CAAA,GAAA,KAAA,MAAA,EAAA;AACA,YAAA,MAAA,CAAA,UAAA,GAAA,IAAA;AACA,YAAA,UAAA,CAAA,MAAA,CAAA,IAAA,EAAA,IAAA,CAAA;AACA,WAHA,MAGA;AACA,YAAA,MAAA,CAAA,gBAAA,GAAA,MAAA,CAAA,WAAA;AACA;AACA,SAlBA;AAmBA,OApBA,MAoBA,IAAA,KAAA,QAAA,KAAA,MAAA,EAAA;AACA,aAAA,gBAAA,GAAA,KAAA,WAAA;AACA,OAFA,MAEA;AACA,aAAA,gBAAA,GAAA,KAAA,WAAA;AACA;AACA,KAjDA;AAkDA,IAAA,gBAlDA,4BAkDA,gBAlDA,EAkDA;AACA,WAAA,gBAAA,GAAA,gBAAA;AACA,KApDA;AAqDA,IAAA,IArDA,kBAqDA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,QAAA;AACA;AAvDA;AAfA,CAAA","sourcesContent":["<template>\r\n    <div class=\"container-fluid\" style=\"padding: 0rem;\">\r\n        <v-nav v-show=\"!afterLogin\"></v-nav>\r\n        <div v-show=\"!afterLogin\">\r\n        <div class=\"row\" style=\"margin-top: 3rem;\">\r\n            <div class=\"col\" style=\"margin-top: 2.5rem;margin: 0 auto;\">\r\n                <h1>注册页面</h1>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"col\" style=\"margin-top: 1.5rem;\">\r\n                <h5>仔细填写，不可更改</h5>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\" style=\"margin: 0 auto;width: 12.5rem;margin-top: 1rem;\">\r\n            <label>填写邮箱</label>\r\n            <b-form-input class=\"col\" type=\"email\" style=\"margin: 0 auto;\" placeholder=\"请填写邮箱地址\" required @blur=\"checkEmail\" v-model=\"getEmail\"></b-form-input>\r\n            <b-alert show v-show=\"showemail\" style=\"margin-top: 0.5rem;\">{{checkMsg}}</b-alert>\r\n        </div>\r\n        <div class=\"row\" style=\"margin: 0 auto;width: 12.5rem;margin-top: 1rem;\">\r\n            <label>填写昵称</label>\r\n            <b-form-input type=\"text\" style=\"margin: 0 auto;\" placeholder=\"请填写昵称\" required v-model=\"nickname\"></b-form-input>\r\n        </div>\r\n        <div class=\"row\" style=\"margin-top: 1rem;\">\r\n            <b-form-group style=\"margin: 0 auto;\" id=\"sex\" label=\"性别\">\r\n                <b-form-radio-group v-model=\"sexChoice\">\r\n                    <b-form-radio value=\"male\">男</b-form-radio>\r\n                    <b-form-radio value=\"female\">女</b-form-radio>\r\n                    <b-form-radio value=\"shemale\">人妖</b-form-radio>\r\n                </b-form-radio-group>\r\n            </b-form-group>\r\n        </div>\r\n        <div class=\"row\" style=\"margin: 0 auto;width: 12.5rem;margin-top: 1rem;\">\r\n            <b-col style=\"margin-top: 1rem;\">\r\n                你选择的是  {{ sexChoice }}\r\n            </b-col>\r\n            </div>\r\n        <div class=\"row\" style=\"margin: 0 auto;width: 12.5rem;margin-top: 1rem;\">\r\n            <label>密码</label>\r\n            <b-form-input type=\"password\" style=\"margin: 0 auto;\" placeholder=\"请填写密码\" required v-model=\"password\"></b-form-input>\r\n        </div>\r\n        <div class=\"row\" style=\"margin: 0 auto;width: 12.5rem;margin-top: 0.5rem;\">\r\n            <b-form-input type=\"password\" style=\"margin: 0 auto;\" placeholder=\"请再次填写密码\" required></b-form-input>\r\n        </div>\r\n        <b-row style=\"margin: 0 auto;margin-top: 0.5rem;width: 30rem;\">\r\n                <b-alert\r\n                      :show=\"dismissCountDown\"\r\n                      dismissible\r\n                      variant=\"danger\"\r\n                      @dismissed=\"dismissCountDown=0\"\r\n                      @dismiss-count-down=\"countDownChanged\"\r\n                      style=\"margin: 0 auto;\"\r\n                    >\r\n                      <p>请完善内容或确认内容无误后再提交</p>\r\n                      <p> {{ dismissCountDown }}秒后警告关闭 </p>\r\n                      <b-progress\r\n                        variant=\"warning\"\r\n                        :max=\"dismissSecs\"\r\n                        :value=\"dismissCountDown\"\r\n                        height=\"4px\"\r\n                      ></b-progress>\r\n                    </b-alert>\r\n        </b-row>\r\n        <div class=\"row\" style=\"margin: 0 auto;margin-top: 0.5rem;width: 6.25rem;\">\r\n            <b-button @click=\"submit()\">确认注册</b-button>\r\n        </div>\r\n        </div>\r\n        <b-container v-show=\"afterLogin\" class=\"text-center\">\r\n            <h1>注册成功</h1>\r\n            <router-link to=\"/login\">点此去登陆</router-link>\r\n        </b-container>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport router from '../router/index.js'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      showemail: false,\r\n      getEmail: null,\r\n      checkMsg: '',\r\n      email: '',\r\n      sexChoice: '',\r\n      nickname: '',\r\n      password: '',\r\n      dismissSecs: 10,\r\n      dismissCountDown: 0,\r\n      afterLogin: false\r\n    }\r\n  },\r\n  methods: {\r\n    checkEmail () {\r\n      axios({\r\n        method: 'post',\r\n        url: this.baseURL() + ':8080' + '/account/query',\r\n        data: {\r\n          email: this.getEmail\r\n        }\r\n      }).then(response => {\r\n        let res = JSON.stringify(response.data.email); this.email = JSON.parse(res); console.log(this.email); console.log(this.getEmail); if (this.email === this.getEmail) {\r\n          this.checkMsg = '已被注册'\r\n        } else {\r\n          let reg = /\\w+@\\w+\\.com$/\r\n          if (reg.test(this.getEmail)) {\r\n            this.checkMsg = '可以注册'\r\n          } else { this.checkMsg = '邮箱格式不对' }\n        }\r\n      })\r\n      if (this.getEmail !== '') { this.showemail = true } else {\r\n        this.showemail = false\r\n      }\r\n    },\r\n    submit () {\r\n      this.checkEmail()\r\n      if (this.nickname !== '' && this.email !== '' && this.sex !== '' && this.password !== '' && this.checkMsg !== '已被注册') {\r\n        axios({\r\n          method: 'post',\r\n          url: this.baseURL() + ':8080' + '/account/add',\r\n          data: {\r\n            nickname: this.nickname,\r\n            email: this.getEmail,\r\n            password: this.password,\r\n            sex: this.sexChoice\r\n          }\r\n        }).then(response => {\r\n          let res = ''\r\n          res = response.data\r\n          if (res.msg === '注册成功') {\r\n            this.afterLogin = true\r\n            setTimeout(this.push, 3000)\r\n          } else {\r\n            this.dismissCountDown = this.dismissSecs\r\n          }\r\n        })\r\n      } else if (this.checkMsg === '已被注册') {\r\n        this.dismissCountDown = this.dismissSecs\r\n      } else {\r\n        this.dismissCountDown = this.dismissSecs\r\n      }\r\n    },\r\n    countDownChanged (dismissCountDown) {\r\n      this.dismissCountDown = dismissCountDown\r\n    },\r\n    push () {\r\n      router.push('/login')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n    .row {\r\n        margin: 0 auto;\r\n    }\r\n    h1 {\r\n        text-align: center;\r\n        margin-top: 2rem;\r\n    }\r\n    h5 {\r\n        text-align: center;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/views"}]}