QAAA,KAAA,CAAA;AACA,UAAA,MAAA,EAAA,KADA;AAEA,UAAA,GAAA,EAAA,KAAA,OAAA,KAAA,OAAA,GAAA,mBAFA;AAGA,UAAA,MAAA,EAAA;AACA,YAAA,UAAA,EAAA,KAAA,QADA;AAEA,YAAA,aAAA,EAAA,KAAA;AAFA;AAHA,SAAA,CAAA,CAOA,IAPA,CAOA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,MAAA;AACA,SAXA;AAYA,aAAA,SAAA,GAAA,IAAA;AACA;AACA,KAxBA;AAyBA,IAAA,MAzBA,oBAyBA;AAAA;;AACA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,GAAA,EAAA,KAAA,OAAA,KAAA,OAAA,GAAA,mBAFA;AAGA,QAAA,MAAA,EAAA;AACA,UAAA,WAAA,EAAA,KAAA;AADA;AAHA,OAAA,CAAA,CAMA,IANA,CAMA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAVA,EAUA,KAVA,CAUA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OAVA;AAWA;AArCA,GA3BA;AAkEA,EAAA,KAAA,EAAA;AACA,IAAA,WAAA,EAAA;AADA;AAlEA,CAAA","sourcesContent":["<template>\r\n    <div class=\"container-fluid\">\r\n        <v-nav></v-nav>\r\n        <b-row style=\"margin-top: 10rem;\">\r\n            <b-input-group class=\"mx-auto\" style=\"width: 50%;\">\r\n                <b-dropdown :text=\"selected\" variant=\"success\">\r\n                    <b-dropdown-item @click=\"change('名称')\">名称</b-dropdown-item>\r\n                    <b-dropdown-item @click=\"change('作者')\">作者</b-dropdown-item>\r\n                    <b-dropdown-item @click=\"change('国家')\">国家</b-dropdown-item>\r\n                </b-dropdown>\r\n                <b-form-input v-model=\"searchContent\" required></b-form-input>\r\n                <b-input-group-addon>\r\n                    <b-button variant=\"info\" @click=\"getComics\">搜索</b-button>\r\n                </b-input-group-addon>\r\n            </b-input-group>\r\n        </b-row>\r\n        <b-row v-show=\"showProps\" style=\"margin-top: 2rem;\" id=\"comicView\">\r\n            <b-col cols=\"4\" v-for=\"(comic,index) in comics\" v-bind:key=\"index\" style=\"margin-top: 2rem;\">\r\n                <b-col cols>\r\n                    <b-img thumbnail fluid left :src=\"comic.cover\" alt=\"没有封面\" blank-color=\"#88f\" width=200 height=300></b-img>\r\n                </b-col>\r\n                <b-row>\r\n                    <b-col>\r\n                        <P>{{comic.comic}}</P>\r\n                        <p>{{comic.type}}</p>\r\n                        <p>{{comic.country}}</p>\r\n                        <p>作者：{{comic.author}}</p>\r\n                    </b-col>\r\n                </b-row>\r\n            </b-col>\r\n        </b-row>\r\n        <b-row style=\"width: 100%;margin-top: 3rem;\" v-show=\"showPage\">\r\n                <div class=\"overflow-auto mx-auto\">\r\n                  <b-pagination\r\n                    v-model=\"currentPage\"\r\n                    :total-rows=\"rows\"\r\n                    :per-page=\"perPage\"\r\n                    aria-controls=\"comicView\"\r\n                  ></b-pagination>\r\n\r\n                  <p class=\"mt-3\">Current Page: {{ currentPage }}</p>\r\n                  </div>\r\n        </b-row>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      showProps: false,\r\n      temperture: 10,\r\n      humidity: 50,\r\n      selected: '名称',\r\n      searchContent: null,\r\n      comics: [],\r\n      perPage: 3,\r\n      currentPage: 1,\r\n      rows: 1,\r\n      showPage: true\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getAll()\r\n  },\r\n  created () {\r\n    axios({\r\n      method: 'get',\r\n      // url: this.baseURL() + ':8080' + '/comics/numbers'\r\n      url: 'http://localhost:8888/comics/numbers'\r\n    }).then(res => {\r\n      this.rows = res.data.comicNumbers\r\n    }).catch(res => console.log(res))\r\n  },\r\n  methods: {\r\n    showProperties () {\r\n      this.showProps = !this.showProps\r\n    },\r\n    change (e) {\r\n      this.selected = e\r\n    },\r\n    getComics () {\r\n      console.log(this.baseURL())\r\n      if (this.searchContent !== null) {\r\n        axios({\r\n          method: 'get',\r\n          url: this.baseURL() + ':8080' + '/comics/getComics',\r\n          params: {\r\n            searchType: this.selected,\r\n            searchContent: this.searchContent\r\n          }\r\n        }).then(res => {\r\n          console.log(res)\r\n          this.comics = res.data\r\n          this.rows = res.length\r\n        })\r\n        this.showProps = true\r\n      }\r\n    },\r\n    getAll () {\r\n      axios({\r\n        method: 'GET',\r\n        url: this.baseURL() + ':8888' + '/comics/pagesList',\r\n        params: {\r\n          pagesNumber: this.currentPage\r\n        }\r\n      }).then(res => {\r\n        this.comics = res.data\r\n        this.showProps = true\r\n        console.log(res)\r\n      }).catch(res => console.log(res))\r\n    }\r\n  },\r\n  watch: {\r\n    currentPage: 'getAll'\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>\n"],"sourceRoot":"src/views"}]}